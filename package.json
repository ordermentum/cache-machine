  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ordermentum/cache-machine/issues"
  },
  "homepage": "https://github.com/ordermentum/cache-machine#readme",
  "keywords": [
    "cache",
    "redis"
  ],
  "peerDependencies": {
    "ioredis": ">=4.9"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/preset-typescript": "7.8.3",
    "@babel/register": "7.8.6",
    "@types/chai": "4.2.9",
    "@types/mocha": "5.2.7",
