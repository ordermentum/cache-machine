  "description": "simple cache built on redis",
  "main": "build/index.js",
  "scripts": {
    "spec": "yarn run mocha -R spec -t 7000 --opts ./mocha.opts",
    "test": "yarn run spec ./test/**/*.ts",
    "build": "tsc --noEmit && babel -Dd build src --extensions \".js,.ts,.tsx\"",
    "prepublish": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ordermentum/cache-machine.git"
  },
