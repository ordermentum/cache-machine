    "chai": "^4.2.0",
    "eslint": "5.16.0",
    "eslint-config-ordermentum": "0.1.0",
    "ioredis": "^4.9.0",
    "lint-staged": "8.2.1",
    "mocha": "^6.0.2",
    "nyc": "10.3.2",
    "prettier": "1.19.1",
    "sinon": "7.2.7",
    "typescript": "3.3.4000",
    "typescript-eslint-parser": "22.0.0",
    "uuid": "^3.3.2"
  }
