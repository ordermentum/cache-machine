    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-ordermentum": "0.1.0",
    "ioredis": "4.16.0",
    "lint-staged": "8.2.1",
    "mocha": "7.0.1",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "sinon": "8.1.1",
    "typescript": "3.8.3",
    "typescript-eslint-parser": "22.0.0",
    "uuid": "3.4.0"
  }
